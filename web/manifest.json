{
  "manifest_version": 2,
  "name": "YouTube Kbd Nav",
  "author": "Philippe Fanaro",
  "version": "1.0.2",
  "description": "YouTube keyboard navigation to the max.",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "exclude_matches": [
        "*://*.studio.youtube.com/*"
      ],
      "js": [
        "content.dart.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite"
  ],
  "icons": {
    "128": "favicon.png"
  }
}